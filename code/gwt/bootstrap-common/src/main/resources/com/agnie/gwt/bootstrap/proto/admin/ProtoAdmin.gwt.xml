<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='ProtoAdmin'>

	<inherits name="org.gwtbootstrap3.GwtBootstrap3" />
	<inherits name="com.googlecode.gwt.charts.Charts" />

	<entry-point class="com.agnie.gwt.bootstrap.proto.admin.client.ProtoAdmin" />

	<source path='client' />
	<source path='shared' />
	<public path='proto' />
	<public path='client/resources'>
		<include name="**/*.css" />
		<include name="**/*.js" />
	</public>
	<stylesheet src="assets/stylesheets/theme.css" />

	<!-- Skin CSS -->
	<stylesheet src="assets/stylesheets/adsirver-theme.css" />

	<!-- Skin CSS -->
	<stylesheet src="assets/stylesheets/skins/adsirver.css" />

	<!-- Theme Custom CSS -->
	<stylesheet src="assets/stylesheets/theme-custom.css" />

	<!-- Head Libs -->
	<script src="assets/vendor/modernizr/modernizr.js" />

</module>
