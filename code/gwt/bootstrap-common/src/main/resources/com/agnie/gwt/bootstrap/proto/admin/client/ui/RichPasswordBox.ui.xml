<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.containerPanel{
		position:relative;
		margin-top:40px;
	}
	</ui:style>
	<g:HTMLPanel addStyleNames="form-group form-material floating {style.containerPanel}">
		<g:PasswordTextBox ui:field="input" addStyleNames="form-control empty"></g:PasswordTextBox>
		<label class="floating-label" ui:field="label"></label>
		
	</g:HTMLPanel>
</ui:UiBinder> 