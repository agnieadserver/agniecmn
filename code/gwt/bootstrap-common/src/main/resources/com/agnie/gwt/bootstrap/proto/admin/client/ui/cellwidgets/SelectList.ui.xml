<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gcw="urn:import:com.google.gwt.user.cellview.client" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b='urn:import:org.gwtbootstrap3.client.ui' xmlns:b.t='urn:import:org.gwtbootstrap3.client.ui.gwt'
	xmlns:p='urn:import:com.agnie.gwt.bootstrap.proto.admin.client.ui'>
	<ui:style>
	.link {
		cursor: pointer;
		text-decoration: none !important;
	}
	
	.cell-list {
		margin-bottom: 0px !important;
	}
	</ui:style>
	<g:HTMLPanel addStyleNames="panel">
		<header class="panel-heading" style="padding-bottom: 0px;">
			<g:Anchor ui:field="clear" addStyleNames="{style.link}">Clear Selection</g:Anchor>
		</header>
		<div class="panel-body">
			<gcw:CellList ui:field="celllist" addStyleNames="{style.cell-list}"
				height="400px">
			</gcw:CellList>
		</div>
		<div class="panel-footer" style="padding-top: 0px;">
			<g:Anchor ui:field="removeAll" addStyleNames="{style.link}">Remove All</g:Anchor>
			<g:Anchor ui:field="removeSelected" addStyleNames="{style.link}">Remove
				Selected</g:Anchor>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 