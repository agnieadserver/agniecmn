<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b='urn:import:org.gwtbootstrap3.client.ui' xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:p='urn:import:com.agnie.gwt.bootstrap.proto.admin.client.ui'
	xmlns:cw='urn:import:com.agnie.gwt.bootstrap.proto.admin.client.ui.cellwidgets'>
	<g:HTMLPanel>
		<section role="main" class="content-body">
			<b:Container fluid="true">
				<b:Row>
					<b:Column size="MD_12">
						<g:HTMLPanel addStyleNames="panel form-wizard" id="w3">
							<header class="panel-heading">
								<div class="panel-actions">
									<a href="#" class="fa fa-caret-down"></a>
									<a href="#" class="fa fa-times"></a>
								</div>

								<h2 class="panel-title">Select Table</h2>
							</header>
							<div class="panel-body">
								<cw:SelectTable ui:field="selectTable"></cw:SelectTable>
							</div>
							<div class="panel-footer">
								<g:Button ui:field="check">Check</g:Button>
								<g:Button ui:field="addSelected">Add Selected</g:Button>
							</div>
						</g:HTMLPanel>
					</b:Column>
				</b:Row>
			</b:Container>
		</section>
	</g:HTMLPanel>
</ui:UiBinder> 