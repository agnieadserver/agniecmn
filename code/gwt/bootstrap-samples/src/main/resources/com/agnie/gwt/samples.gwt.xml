<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='samples'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- <inherits name='com.agnie.gwt.child.child'/> -->
	<inherits name='com.agnie.gwt.bootstrap.proto.admin.ProtoAdmin' />
	<inherits name="org.gwtbootstrap3.extras.datetimepicker.DateTimePicker" />
	<inherits name="org.gwtbootstrap3.extras.slider.Slider" />
	<inherits name="com.agnie.gwt.codemirror.codemirror" />
	<inherits name="com.agnie.gwt.codemirror.addon.edit_closebrackets" />
	<inherits name="com.agnie.gwt.codemirror.mode.htmlmixed" />
	<inherits name="com.agnie.gwt.codemirror.theme.monokai" />
	<inherits name="com.agnie.gwt.codemirror.keymap.vim" />
	<inherits name="com.googlecode.gwt.charts.Charts" />
	<!-- Specify the app entry point class. -->
	<entry-point class='com.agnie.gwt.client.samples' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
</module>
