<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field='res' type='com.agnie.gwt.common.client.widget.GreenButtonResources' />

	<g:HTMLPanel ui:field="panel">
		<div class='{res.css.greenButton}'>
			<table style="border: 0px; border-collapse: collapse;"
				cellpadding="0">
				<tr>
					<td>
						<img src="images/transperent.png" class='{res.css.greenButtonLeft}' />
					</td>
					<td>
						<div class='{res.css.greenButtonBody}'>
							<g:Label ui:field="label"></g:Label>
						</div>
					</td>
					<td>
						<img src="images/transperent.png" class='{res.css.greenButtonRight}' />
					</td>
				</tr>
			</table>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>