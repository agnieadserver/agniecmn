<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:agcw='urn:import:com.agnie.gwt.common.client.widget'>
	<ui:with field='res'
		type='com.agnie.gwt.common.client.widget.SearchBoxResources' />
	<ui:with field='msg'
		type='com.agnie.gwt.common.client.Messages' />
	
	<g:HTMLPanel addStyleNames='{res.css.searchBox}'>
			<agcw:LabelTextBox ui:field="search" label='{msg.search}' addStyleNames='{res.css.textBox}'></agcw:LabelTextBox>
			<g:HTMLPanel ui:field="inputWidgetContainer" addStyleNames='{res.css.inputWidgetContainer}'></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>