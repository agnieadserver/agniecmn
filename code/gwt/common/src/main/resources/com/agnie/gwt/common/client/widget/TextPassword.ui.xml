<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field='res'
		type='com.agnie.gwt.common.client.widget.TextPasswordResources' />

	<g:HTMLPanel >
		<g:TextBox ui:field="textBox" addStyleNames='{res.css.textBox}'></g:TextBox>
		<g:PasswordTextBox ui:field="passBox" addStyleNames='{res.css.passBox}'></g:PasswordTextBox>
	</g:HTMLPanel>
</ui:UiBinder>