<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:at="urn:import:com.agnie.gwt.common.client.widget.tag">
	<ui:with field='res'
		type='com.agnie.gwt.common.client.widget.tag.TagResources' />
	<g:HTMLPanel addStyleNames="{res.css.tageditor}">
		<at:Editor ui:field="editor"></at:Editor>
		<div class="{res.css.readonly}" ui:field="displayreadonly"></div>
	</g:HTMLPanel>
</ui:UiBinder> 