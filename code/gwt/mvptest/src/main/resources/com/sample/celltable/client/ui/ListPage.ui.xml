<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more
		details */
		.cellTable {
		border-bottom: 1px solid #ccc;
		text-align: left;
		margin-bottom: 4px;
		}

		.centercss {
		width: 100%;
		border: black;
		border-style: solid;
		border-width: 1px;
		}

		.btncss {
		margin-top:10px;
		margin-bottom: 10px;
		width: 200px;
		height: 30px;
		}


	</ui:style>



	<b:Panel type="DEFAULT">
		<b:PanelHeader>
			<b:Heading size="H3" text="User Tabel" />
		</b:PanelHeader>
		<b:PanelBody>
			<c:CellTable pageSize='5' ui:field='cellTable'></c:CellTable>
			<c:SimplePager ui:field='pager' />
			<b:Button ui:field="btnadd" type="PRIMARY" text="Add More">
			</b:Button>
		</b:PanelBody>

	</b:Panel>



</ui:UiBinder> 