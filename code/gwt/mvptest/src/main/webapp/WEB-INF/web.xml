<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<servlet>
		<servlet-name>greetservlet</servlet-name>
		<servlet-class>com.sample.celltable.server.GreetingServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>greetservlet</servlet-name>
		<url-pattern>/celltable/greet</url-pattern>
	</servlet-mapping>
	<!-- default page to serve -->
	<welcome-file-list>
		<welcome-file>samples.html</welcome-file>
	</welcome-file-list>





</web-app>
