<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
	<ui:with field='person' />

	<div>
		First Name :
		<span>
			<ui:text from='{person.getFname}' />
		</span>
		<p>
			Last Name :
			<span>
				<ui:text from='{person.getLname}' />
			</span>
		</p>
		<p>
			Email :
			<span>
				<ui:text from='{person.getEmailid}' />
			</span>
		</p>
	</div>
</ui:UiBinder>