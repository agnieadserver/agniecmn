<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:agcw='urn:import:com.agnie.gwt.common.client.widget' >

	
	<g:HTMLPanel>
		<agcw:FormFieldContainer required="true"
			label="First Name">
			<agcw:inputfieldcontainer>
				<g:TextBox></g:TextBox>
			</agcw:inputfieldcontainer>
			<agcw:inputfieldcontainer>
				<g:Button>Test</g:Button>
			</agcw:inputfieldcontainer>
		</agcw:FormFieldContainer>
		<agcw:FormFieldContainer required="false"
			label="Middle Name">
			<agcw:inputfieldcontainer>
				<g:TextBox></g:TextBox>
			</agcw:inputfieldcontainer>
		</agcw:FormFieldContainer>
		<agcw:FormFieldContainer required="true"
			label="Last Name" error="LastName is required field">
			<agcw:inputfieldcontainer>
				<g:TextBox></g:TextBox>
			</agcw:inputfieldcontainer>
		</agcw:FormFieldContainer>
	</g:HTMLPanel>
</ui:UiBinder>