                <g:TextBox></g:TextBox>
            </agcw:inputfieldcontainer>
        </agcw:FormFieldContainer>
        <agcw:FormFieldContainer required="true" label="CustomListBox">
            <agcw:inputfieldcontainer>
                <g:HTMLPanel ui:field="custListPan"></g:HTMLPanel>
